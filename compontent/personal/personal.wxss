/* compontent/personal/personal.wxss */
.personal{
  width: 100%;
  height: 1206rpx;
  background: #CCCCCC;
}
.header{
  width: 100%;
  height: 350rpx;
  background: white;
  border-bottom: 2rpx solid #DDDDDD; 
  padding-top: 20rpx;
}
.header>image{
  width: 164rpx;
  height: 206rpx;
  display: block;
  margin: auto;
}
.header>view:nth-of-type(1){
  width: 164rpx;
  height: 50rpx;
  font-size: 35rpx;
  margin: auto;
  font-weight: bold;
}
.header>view:nth-of-type(1)>text:nth-of-type(2){
  margin-left: 35rpx;
}
.header>view:nth-of-type(2){
  width: 100rpx;
  height: 50rpx;
  font-size: 35rpx;
  margin: auto;
  line-height: 50rpx;
  margin-top: 20rpx;
}
.two{
  width: 100%;
  height: 103rpx;
  background: white;
  padding-left: 30rpx;
  font-size: 30rpx;
  line-height: 103rpx;
  display: flex;
}
.two>image{
  width: 20rpx;
  height: 31rpx;
  display: block;
  margin-left: 550rpx;
  margin-top: 40rpx;
}